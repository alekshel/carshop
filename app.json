{
  "name": "CarShop on Heroku",
  "description": "E-Commerce app based on Django",
  "image": "heroku/python",
  "repository": "https://github.com/alekshel/carshop",
  "keywords": ["python", "django"],
  "env": {
    "DJANGO_SECRET_KEY": {
      "description": "The secret key for the Django application.",
      "generator": "secret"
    }
  },
  "environments": {
    "test": {
      "scripts": {
        "test": "./manage.py test --debug-mode"
      }
    }
  }
}
